    AUI().ready('aui-toggler', function (A) {
        var obj = document.getElementsByClassName('taglib-search-toggle-advanced-wrapper');
        if (obj && obj.length > 0) {
            obj[0].style.display = 'block';
        }
    });

    $('#_com_liferay_site_navigation_language_web_portlet_SiteNavigationLanguagePortlet_fm').change(function () {
        var text = $(this).find('option:selected').text()
        var $aux = $('<select/>').append($('<option/>').text(text))
        $(this).after($aux)
        $(this).width($aux.width())
        $aux.remove()
    }).change()

