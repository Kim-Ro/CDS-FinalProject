                    <script>
                        var toogleMobileSearch = function () {
                            AUI().use('node', function (Y) {
                                var searchPortletHeader = document.querySelector(".search-portlet-header");
                                displayStyleSearchPortletHeader = window.getComputedStyle(searchPortletHeader);
                                displayStyle = displayStyleSearchPortletHeader.getPropertyValue('display');

                                if(displayStyle == "none"){
                                    searchPortletHeader.style.display = 'block';
                                }else{
                                    searchPortletHeader.style.display = 'none';
                                }
                            });
                        };
                    </script>
