    AUI().use('aui-node', function (A) {
        A.all('a.download').on('click', function (e) {
            var node = A.one(e.currentTarget);
            var dataUri = node.getAttribute('data-uri');
            e.preventDefault();
            if (!dataUri) {
                dataUri = node.getAttribute('href');
            }
            window.location.href = dataUri;
        });
    });
